{
  "name": "@file-explorer",
  "version": "0.1.0",
  "description": "A generic file-explorer for rendering file trees",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [
    "git",
    "diff",
    "react",
    "typescript",
    "viewer",
    "addons"
  ],
  "author": "Edgar Fernandes",
  "license": "AGPL-3.0-only",
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@test-lib": "workspace:*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/coverage-v8": "3.2.4",
    "jsdom": "^26.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@commons": "workspace:*",
    "@emotion/react": "^11.14.0",
    "antd": "^5.26.2"
  }
}
